    "watchify": "^3.11.0"
  },
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "babel-preset-es2015": "^6.24.1"
  }
